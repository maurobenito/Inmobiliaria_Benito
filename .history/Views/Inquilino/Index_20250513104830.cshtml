@model IEnumerable<Inmobiliaria_Benito.Models.Inquilino>

<h2>Listado de Inquilinos</h2>

<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>DNI</th>
            <!-- Agregá más campos si querés -->
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Nombre</td>
            <td>@item.Apellido</td>
            <td>@item.Dni</td>
        </tr>
    }
    </tbody>
</table>
