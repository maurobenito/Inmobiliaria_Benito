@model Inmobiliaria_Benito.Models.Inmueble

@{
    ViewData["Title"] = "Crear Inmueble";
}

<h2>Crear Inmueble</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Direccion" class="control-label"></label>
        <input asp-for="Direccion" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Uso" class="control-label"></label>
        <input asp-for="Uso" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="IdTipo" class="control-label"></label>
        <input asp-for="IdTipo" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="IdPropietario" class="control-label"></label>
        <input asp-for="IdPropietario" class="form-control" />
    </div>
   
    </div>
    <button type="submit" class="btn btn-primary">Crear</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>
